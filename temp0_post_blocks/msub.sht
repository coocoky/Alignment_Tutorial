#!/bin/sh

# Purpose:
# For each layer in range, cd to montage dir, run lsq there.
#
# > ./msub.sht <zmin> [zmax]


if (($# == 1))
then
	last=$1
else
	last=$2
fi

for lyr in $(seq $1 $last)
do
	echo $lyr
	if [ -d "$lyr" ]
	then
		cd $lyr/montage

		QSUB_1NODE.sht "mon-$lyr" "" 1 "./runlsq.sht"

		cd ../..
	fi
done

